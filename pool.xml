<?xml version="1.0" encoding="UTF-8"  standalone="yes"?>
<Domus>

<calendar name="PompeCalendar" file="pompe.ca"/>
<calendar name="RobotCalendar" file="robot.ca"/>
<calendar name="LumiereCalendar" file="lumiere.ca"/>

<ScheduledGPIO>
	<InversedRelay>
		<name>Pompe</name>
		<gpio>GPIO_00</gpio>
		<calendarFile>PompeCalendar</calendarFile>
	</InversedRelay>
	<InversedRelay>
		<name>Robot</name>
		<gpio>GPIO_06</gpio>
		<calendarFile>RobotCalendar</calendarFile>
	</InversedRelay>
	<InversedRelay>
		<name>Lumiere</name>
		<gpio>GPIO_05</gpio>
		<calendarFile>LumiereCalendar</calendarFile>
	</InversedRelay>
</ScheduledGPIO>

</Domus>